<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                  assembly="INTUSOFT.Data"
                  namespace="INTUSOFT.Data.Model">

  <class name="ImageModel" table="Image">
    <id name="ID">
      <column name=" ImageID " />
      <generator class="native" />
    </id>
    <property name="HideShowRow"  not-null ="false"/>
    <property name="Comments"  not-null ="false" />
    <property name="CameraSettings"  not-null ="false" />
    <property name="ImageDateTime"  not-null ="false" />
    <property name="isDilation" not-null ="false"/>
    <property name="EyeType" column="EyeType" not-null ="false"/>
    <property name="EyeSide"   not-null ="false"/>
    <property name="LocalURL" column ="LocalURL" not-null ="false"/>
    <property name="ServerURL" column ="ServerURL" not-null ="false"/>
    <property name="ImageName"  not-null ="false"/>
    <property name="VisitID" column="Image_visitId"   not-null ="false" />

  </class>

</hibernate-mapping>
